{
  "name": "my-express-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/ --fix",
    "test": "env NODE_ENV=test nyc jest",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=lcov",
    "build": "npx babel src -d lib",
    "build:watch": "npx babel src -d lib --watch",
    "start": "env NODE_ENV=production node lib/index.js",
    "dev": "env NODE_ENV=development nodemon lib/index.js",
    "db:create": "npx sequelize db:create",
    "db:migrate": "npx sequelize db:migrate:undo:all && npx sequelize db:migrate",
    "seed": "npx sequelize db:seed:undo:all && npx sequelize db:seed:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "coveralls": "^3.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "nyc": "^15.1.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "regenerator-runtime": "^0.13.7",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1"
  }
}
